{
  "name": "generate-react-spa",
  "version": "1.0.0",
  "description": "webpack react spa ",
  "main": "index.js",
  "author": "sam.lee",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev2": "webpack-dev-server --config ./build/webpack.dev.conf  --content-base assets/ --output-public-path static/ --progress --colors --inline --history-api-fallback ",
    "dev": "NODE_ENV=development&&webpack-dev-server --config ./build/webpack.dev.conf --progress --colors  --inline ",
    "build": "NODE_ENV=production && webpack --config ./build/webpack.prod.conf&&sh publish.sh",
    "build-dev": "NODE_ENV=development&&webpack --config ./build/webpack.prod.conf&&sh publish-dev.sh",
    "prod": "webpack --config ./webpack.dev.config"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.3",
